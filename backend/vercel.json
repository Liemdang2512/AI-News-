{
    "version": 2,
    "builds": [
        {
            "src": "backend/index.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "backend/index.py"
        }
    ]
}